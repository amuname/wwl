	"description": "Запускает WhatsAppWeb по клику на номер телефона",
	"version": "1.0.0",
	"icons": {"128": "icon.jpg"},
	"content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "content.js" ]
    }
  ],
	"background": { 
		"scripts" : ["background.js"], 
	"persistent" : true 
	},
	"browser_action": {
		"default_title": "WhatsAppWeb Launcher β",
		"default_icon": "icon.jpg",
		"default_popup": "popup.html"
	},
	"permissions": ["activeTab","storage"]
}