{
	"manifest_version": 2,
	"name": "WhatsAppWeb Launcher β",
	// "externally_connectable": {

 //    "ids": [
 //      "*"
 //    ],
 //    "matches": [
 //      "*://web.whatsapp.com/*",
 //      "*://app.syncrm.ru/*"
 //    ]
